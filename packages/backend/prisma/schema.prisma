// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://root:root@localhost:5432/lezzoo-task"
}

model Location {
  id        Int     @id @default(autoincrement())
  longitude Float
  latitude  Float
}
